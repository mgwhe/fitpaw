<div class="col-sm-12">
  <div class="list-container">
      <div class="petprofile">
        <p>
          <span class="item-label">Pet Name:</span>
          <%= petprofile_variable.petName %>
        </p>
        <p>
          <span class="item-label">Pet Age:</span>
          <%= petprofile_variable.petAge %>
        </p>
        <p>
          <span class="item-label">Pet Weight (kg):</span>
          <%= petprofile_variable.petWeight %>
        </p>
        <p>
          <span class="item-label">Pet Tag Number:</span>
          <%= petprofile_variable.petTagNumber %>
        </p>
      </div>
  </div>
</div>
</div>